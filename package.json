{
  "name": "@metricinsights/cs-helper",
  "version": "0.4.0-beta.1",
  "description": "Helper for Custom Scripts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:sources && npm run build:bin",
    "postbuild": "npm pack",
    "build:sources": "tsc",
    "build:bin": "tsc --project tsconfig.bin.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "semantic-release",
    "version": "npm version --commit-hooks false --git-tag-version false"
  },
  "bin": {
    "cs-helper": "dist/bin/build.js",
    "cs-helper-create": "dist/bin/create.js"
  },
  "files": [
    "dist/",
    "templates",
    "README.md",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mi-examples/cs-helper.git"
  },
  "keywords": [
    "helper",
    "custom script",
    "Metric Insights",
    "MI",
    "MetricInsights"
  ],
  "author": "Serhii Shpak",
  "license": "ISC",
  "overrides": {
    "tar": ">=7.5.7",
    "npm": {
      "tar": ">=7.5.7"
    }
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jquery": "^3.5.16",
    "@types/promise-polyfill": "^6.0.6",
    "@types/prompts": "^2.4.3",
    "prettier": "^3.8.1",
    "semantic-release": "^25.0.2"
  },
  "dependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.10",
    "@babel/preset-typescript": "^7.26.10",
    "babel-loader": "^9.1.2",
    "chalk": "^5.2.0",
    "commander": "^10.0.0",
    "isbinaryfile": "^5.0.0",
    "promise-polyfill": "8.3.0",
    "prompts": "^2.4.2",
    "string-includes-polyfill": "^1.0.6",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.2",
    "webpack": "^5.76.2"
  },
  "bugs": {
    "url": "https://github.com/mi-examples/cs-helper/issues"
  },
  "homepage": "https://github.com/mi-examples/cs-helper#readme"
}
